<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/naval"
    tools:context=".Activity_resultado_final">

    <TextView
        android:id="@+id/tv_resultado"
        android:layout_width="199dp"
        android:layout_height="70dp"
        android:layout_marginTop="72dp"
        android:text="preço"
        android:textColor="@color/white"
        android:textSize="52dp"
        android:textStyle="bold"
        android:gravity="center"
        android:maxLength="4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_label_revisao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="clip_horizontal"
        android:layout_marginTop="60dp"
        android:text="Revisão dos dados inseridos"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="@+id/tv_resultado"
        app:layout_constraintStart_toStartOf="@+id/tv_resultado"
        app:layout_constraintTop_toBottomOf="@+id/tv_resultado" />

    <TextView
        android:id="@+id/tv_label_preco"
        android:layout_width="81dp"
        android:layout_height="30dp"
        android:layout_gravity="clip_horizontal"
        android:layout_marginTop="92dp"
        android:text="Preço"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.109"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_label_revisao" />

    <TextView
        android:id="@+id/tv_label_consumo"
        android:layout_width="90dp"
        android:layout_height="33dp"
        android:layout_gravity="clip_horizontal"
        android:layout_marginStart="4dp"
        android:layout_marginTop="64dp"
        android:text="Consumo"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/tv_label_preco"
        app:layout_constraintTop_toBottomOf="@+id/tv_label_preco" />

    <TextView
        android:id="@+id/tv_label_km"
        android:layout_width="37dp"
        android:layout_height="25dp"
        android:layout_gravity="clip_horizontal"
        android:layout_marginStart="4dp"
        android:layout_marginTop="64dp"
        android:text="KM"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@+id/tv_label_consumo"
        app:layout_constraintTop_toBottomOf="@+id/tv_label_consumo" />

    <TextView
        android:id="@+id/tv_preco_result"
        android:layout_width="67dp"
        android:layout_height="33dp"
        android:layout_marginTop="92dp"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/tv_label_preco"
        app:layout_constraintTop_toBottomOf="@+id/tv_label_revisao"
        tools:text="1.7" />

    <TextView
        android:id="@+id/tv_consumo_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="172dp"
        android:layout_marginTop="64dp"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_preco_result"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_label_consumo"
        app:layout_constraintTop_toBottomOf="@+id/tv_preco_result"
        tools:text="5.0" />

    <TextView
        android:id="@+id/tv_km_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="212dp"
        android:layout_marginTop="64dp"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_consumo_result"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_label_km"
        app:layout_constraintTop_toBottomOf="@+id/tv_consumo_result"
        tools:text="160" />
</androidx.constraintlayout.widget.ConstraintLayout>